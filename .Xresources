
! Transparency 
! True transparency
!URxvt*.depth: 24
! Native transparency
URxvt*.transparent: true
URxvt*.shading: 66

! Font
!URxvt*.font: 9x15
URxvt*.font: -xos4-terminus-medium-r-normal--14-140-72-72-c-80-iso8859-9

! Colors
*foreground:#D2C5BC 
!*background:[50]#101010
*background:#101010
*.cursorColor:	   #808080

*color0:           #424242
*color8:           #363636

*color1:           #DC322F
*color9:           #CB2C16

*color2:           #479900
*color10:          #587558

*color3:           #B58900
*color11:          #837C65

*color4:           #268BD2
*color12:          #628295

*color5:           #D33682
*color13:          #C46CBD

*color6:           #2AA198
*color14:          #93A1A1

*color7:           #FFF0F0
*color15:          #FDF6E3


! True: enable the scrollbar [default]; option -sb. False: disable
URxvt*.scrollBar: false

URxvt*.perl-ext-common: default,matcher

! URLs
URxvt*.url-launcher: /usr/bin/xdg-open
URxvt*.matcher.button: 1
URxvt*.colorUL: #4682B4

